{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "app": "./app.json",
        "constants": "./src/constants",
        "styles": "./src/styles"
      }
    }]
  ]
}